{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "baseUrl": "./src" /* Base directory to resolve non-absolute module names. */,
    "paths": {
      "@app/*": ["app/*"],
      "@domains/*": ["domains/*"],
      "@assets/*": ["app/assets/*"],
      "@components/*": ["app/components/*"],
      "@hooks/*": ["app/hooks/*"],
      "@navigation/*": ["app/navigation/*"],
      "@context/*": ["app/context/*"]
    },
  }
}
